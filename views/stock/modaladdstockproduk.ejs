<!-- Modal modal-lg -->
<div class="modal fade" id="modalklikadddata" tabindex="-1" aria-labelledby="modalklikadddatalabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable ">
        <div class="modal-content">
            <div class="modal-header">
               
                <h1 class="modal-title fs-5" id="modalklikadddatalabel" style="font-weight: bold;vertical-align: middle;">Tambah Data</h1>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                
                <div class="d-lg-flex align-items-center justify-content-lg-start">
                    <div class="form-check me-3">
                        <input class="form-check-input" type="radio" value="" id="incek" name="inoroutradio" onclick="cektombolinorout(this)">
                        <label class="form-check-label" for="incek" >
                            Tambah Masuk
                        </label>
                    </div>
                    <div class="form-check">
                        <input class="form-check-input" type="radio" value="" id="outcek" name="inoroutradio" onclick="cektombolinorout(this)">
                        <label class="form-check-label" for="outcek">
                            Tambah Keluar
                        </label>
                    </div>
                </div>

                <div class="form-floating mb-3 mt-3">
                    <select class="form-select form-select-sm" id="dataitemselek" aria-label="Floating dataitemselek" onchange="selekitemdata(this)">
                        
                        <!-- <option value="1">One</option>
                        <option value="2">Two</option>
                        <option value="3">Three</option> -->
                    </select>
                    <label for="dataitemselek">Data Item</label>
                </div>
                
                <div class="form-floating mb-3">
                    <input type="number" class="form-control form-control-sm" id="qtyitem" min="0" disabled>
                    <label for="qtyitem">Jumlah</label>
                </div>

                <div class="form-floating mb-3">
                    <textarea class="form-control" id="keteranganinput"></textarea>
                    <label for="keteranganinput">Keterangan</label>
                </div>

                <p style="font-weight: bold;">Upload foto/video bukti</p>
                <div class="d-flex align-items-center justify-content-start">
                    <div class="form-check me-3">
                        <input class="form-check-input" type="radio" value="" id="fotocek" name="fotoorvideoradio" onclick="cekfotoorvideo(this)">
                        <label class="form-check-label" for="fotocek">
                            Foto
                        </label>
                    </div>
                    <div class="form-check">
                        <input class="form-check-input" type="radio" value="" id="videocek" name="fotoorvideoradio" onclick="cekfotoorvideo(this)">
                        <!-- onclick="cekfotoorvideo(this)" -->
                        <label class="form-check-label" for="videocek">
                            Video
                        </label>
                    </div>
                </div>
                
                <div class="mt-1 row" id="showupload-foto" style="display: none;">
                    <div class="col-2">
                        <div class="d-flex justify-content-start h-100" title="Gallery">
                            <button type="button" class="btn btn-light" id="img-klikpload" onclick="klikuploadimgorvid(this)"><i class="bi bi-image"></i></button>
                          
                        </div>
                        
                    </div>
                    <div class="col-2">
                        <div class="d-flex justify-content-start h-100" title="kamera">
                            <button type="button" class="btn btn-light" id="kamerafoto-klikpload" onclick="klikuploadimgorvid(this)"><i class="bi bi-camera-fill"></i></button>
                           
                        </div>
                    </div>
                    <div >
                        <div class="p-2">
                            <span id="img-urlupload" style="font-size: xx-small;"></span>
                        </div>
                       
                    </div>
                    <input class="form-control form-control-sm" id="foto-upload" type="file" accept="image/*" style="display: none;" onclick="this.value=null;">
                    <input class="form-control form-control-sm" id="kamerafoto-upload" type="file" style="display: none;" accept="image/*" capture onclick="this.value=null;">
                </div>

                <div class="mt-1 row" id="showupload-video" style="display: none;">
                    <div class="col-2">
                        <div class="d-flex justify-content-start h-100">
                            <button type="button" class="btn btn-light" id="vid-klikpload" onclick="klikuploadimgorvid(this)"><i class="bi bi-film"></i></button>
                           
                        </div>
                    </div>
                    <div class="col-2">
                        <div class="d-flex justify-content-start h-100" title="kamera">
                            <button type="button" class="btn btn-light" id="kameravideo-klikpload" onclick="klikuploadimgorvid(this)"><i class="bi bi-camera-fill"></i></button>
                           
                        </div>
                    </div>
                    <div>
                        <div class="p-2">
                            <span id="vid-urlupload" style="font-size: xx-small;"></span>
                        </div>
                        
                    </div>
                    <input class="form-control form-control-sm" id="video-upload" type="file" accept="video/*" style="display: none;" onclick="this.value=null;">
                    <input class="form-control form-control-sm" id="kameravideo-upload" type="file" style="display: none;" accept="video/*" capture onclick="this.value=null;">
                </div>

                <div style="display: none;">
                    <video id="video" autoplay playsinline></video>
                    <canvas id="canvas"></canvas>
                </div>
                
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" id="submitombol">Submit</button>
                <button type="button" class="btn btn-light" style="border-color: grey;"  data-bs-dismiss="modal">Batal</button>
                
            </div>
        </div>
    </div>
</div>