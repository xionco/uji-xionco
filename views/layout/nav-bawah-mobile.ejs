<% var homeopen="bottom-bar-normal"; var adminopen='bottom-bar-normal'; var procurementopen='bottom-bar-normal';var financeopen='bottom-bar-normal';var akunopen='bottom-bar-normal';var omnibusformopen='bottom-bar-normal'; %>
<% if (navatasactive=='home') { %>
    <% homeopen='bottom-bar-kliked'; %>
<% } else if (navatasactive=='adminlist') { %>
 
    <% adminopen='bottom-bar-kliked'; %>
<% } else if (navatasactive=='procurement') { %>
    <% procurementopen='bottom-bar-kliked'; %>

<% } else if (navatasactive=='finance') { %>
    <% financeopen='bottom-bar-kliked'; %>
<% } else if (navatasactive=='akun') { %>
    <% akunopen='bottom-bar-kliked'; %>
<% } else if (navatasactive=='omnibusform') { %>
    <% omnibusformopen='bottom-bar-kliked'; %>

<% } %>

<nav class="navbar fixed-bottom bg-light" id="navbarbawah">
    <!-- style="background-color: rgb(236, 236, 236)!important;" -->
    <div class="container-fluid justify-content-center">
        <div class="col justify-content-center">
            <div id="progressbarload" style="margin-bottom: 4px;display: none;" class="row">
                <div class="col">
                    <div class="progress">
                        <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 0%;"></div>
                    </div>
                    <!-- <progress id="progressvalue" value="80" max="100" style="width: 100%;"></progress> -->
                </div>
                
            </div>
            
            <div class="row justify-content-center" >
                <div class="col-2 <%-adminopen %> d-flex align-items-center justify-content-center" id="procurementtombolshow" title="ADMIN" onclick="window.open('/adminlist','_self')">
                    <div class="col" id="procurementtombol">
                        <i class="bi bi-bag-plus" style="font-size: 30px;text-align: center;"></i>
                        <p style="font-size: smaller;text-align: center;">ADMIN</p>
                    </div>
                </div>
                <div class="col-2 <%-procurementopen%> d-flex align-items-center justify-content-center" style="margin-left: 30px!important;text-align: center!important;" id="procurementtombolshow" title="PROCUREMENT">
                    <div class="col" id="procurementtombol">
                        <i class="bi bi-box" style="font-size: 30px;text-align: center;"></i>
                        <p style="font-size: smaller;">PROC</p>
                    </div>
                </div>
                <div class="col-2 <%-omnibusformopen%> d-flex align-items-center justify-content-center" style="margin-left: 30px!important;" id="omnibusform" title="omnibus form" onclick="window.open('/omnibusform','_self')">
                    <div class="col" id="statustombol">
                        <i class="bi bi-journal-plus" style="font-size: 30px;text-align: center;"></i>
                        <p style="font-size: smaller;">FORM</p>
                    </div>
                </div>
                <div class="col-2 <%-akunopen%> d-flex align-items-center justify-content-center" style="margin-left: 30px!important;" title="AKUN">
                  <div class="col" id="akuntombol">
                      <i class="bi bi-person-circle" style="font-size: 30px;text-align: center;"></i>
                      <p style="font-size: smaller;">AKUN</p>
                  </div>
                </div>
              </div>
        </div>
      
    </div>
</nav>